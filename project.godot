; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GameJam15"
run/main_scene="res://src/scenes/menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Globals="*res://resources/globals.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"BadGuy": "res://assets/Pixel Adventure/Main Characters/BadGuy.dch",
"Me": "res://assets/Dialogic/Me.dch"
}
directories/dtl_directory={
"incorrect_coin_instruction": "res://assets/Dialogic/incorrect_coin_instruction.dtl",
"timetravel_reveal": "res://assets/Dialogic/timetravel_reveal.dtl",
"timetravel_start_chat": "res://assets/Dialogic/timetravel_start_chat.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://assets/Dialogic/chat_style.tres"]
layout/default_style="res://assets/Dialogic/chat_style.tres"
variables={}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
portraits/default_portrait=""
animations/join_default="Fade In Up"
text/autoadvance_enabled=true
text/autoadvance_fixed_delay=1.0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"
window/transparent_background=false
window/defaults/default_clear_color=Color(0.3, 0.3, 0.3, 1)

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

